                                            <button type="button" class="btn btn-danger btn-wd pull-left"
                                                    onclick="utilities.showPhoneTable()">
                                                CANCEL
                                            </button>
                                        </div>
                                        <div class="col">
                                            <button type="button" id="updateBtn"
                                                    class="btn btn-danger btn-wd pull-right"
                                                    onclick="utilities.updatePhone()">
                                                UPDATE
                                            </button>
                                            <button type="button" id="saveBtn"
                                                    class="btn btn-danger btn-wd pull-right"
                                                    onclick="utilities.addPhone()">
                                                SAVE
                                            </button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
