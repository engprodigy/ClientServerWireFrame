                    <div class="row justify-content-center">
                        <div class="col-xs-12">
                            <div id="email-toolbar">
                                <button class="btn btn-danger" type="button"
                                        onclick="utilities.showNewEmailFrm()">
                                    <i class="now-ui-icons ui-1_simple-add"></i>
                                    Add new email
                                </button>
                            </div>
