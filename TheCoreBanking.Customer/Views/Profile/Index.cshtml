<style>
    .form-check .form-check-radio {
        display: inline-block;
        position: relative;
        padding: 0 6px;
        margin: 10px 0 0;
    }

        .form-check .form-check-radio input[type='radio'] {
            display: none;
        }

        .form-check .form-check-radio form-check-label {
            color: #666;
            font-weight: normal;
        }

    .form-check form-check-radio form-check-label:before {
        content: " ";
        display: inline-block;
        position: relative;
        top: 5px;
        margin: 0 5px 0 0;
        width: 20px;
        height: 20px;
        border-radius: 11px;
        border: 2px solid #004c97;
        background-color: transparent;
    }

    .form-check .form-check-radio input[type=radio]:checked + form-check-label:after {
        border-radius: 11px;
        width: 12px;
        height: 12px;
        position: absolute;
        top: 9px;
        left: 10px;
        content: " ";
        display: block;
        background: #004c97;
    }


    .radio-item {
        display: inline-block;
        position: relative;
        padding: 0 6px;
        margin: 10px 0 0;
    }

        .radio-item input[type='radio'] {
            display: none;
        }

        .radio-item label {
            color: #666;
            font-weight: normal;
        }

            .radio-item label:before {
                content: " ";
                display: inline-block;
                position: relative;
                top: 5px;
                margin: 0 5px 0 0;
                width: 20px;
                height: 20px;
                border-radius: 11px;
                border: 2px solid red;
                background-color: transparent;
            }

        .radio-item input[type=radio]:checked + label:after {
            border-radius: 11px;
            width: 12px;
            height: 12px;
            position: absolute;
            top: 9px;
            left: 10px;
            content: " ";
            display: block;
            background: red;
        }

        .modal-dialog,
.modal-content {
    /* 80% of window height */
   /* height: 80%;*/
}

    .modal-body {
        /* 100% = dialog height, 120px = header + footer 
        max-height: calc(100% - 120px);*/
       
      //  overflow-y: scroll;
    }
</style>

